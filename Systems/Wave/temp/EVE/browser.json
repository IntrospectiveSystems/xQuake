{
  "Sources": {
    "xGraph": "{xGraph}",
    "xVR": "{xVR}"
  },
  "Modules": {
    "Deferred": [
      {
        "Module": "xGraph.Popup",
        "Source": "xGraph"
      },
      {
        "Module": "xGraph.Popup",
        "Source": "xGraph"
      }
    ],
    "RootView": {
      "Module": "xGraph.RootView",
      "Source": "xGraph",
      "Par": {
        "Layout": "$View3D"
      }
    },
    "Mouse": {
      "Module": "xVR.Web.Mouse",
      "Source": "xVR",
      "Par": {
        "Handler": "$View3D"
      }
    },
    "Model3D": {
      "Module": "xVR.Web.Model3D",
      "Source": "xVR",
      "Par": {}
    },
    "ServerProxy": {
      "Module": "xGraph.WebProxy",
      "Source": "xGraph",
      "Par": {
        "Link": "$View3D",
        "Role": "Server",
        "Chan": "View3D"
      }
    },
    "ControllerProxy": {
      "Module": "xGraph.WebProxy",
      "Source": "xGraph",
      "Par": {
        "Chan": "Controller",
        "Role": "Client"
      }
    },
    "View3D": {
      "Module": "xVR.Web.View3D",
      "Source": "xVR",
      "Par": {
        "Navigation": "$Mouse",
        "Controller": "$ControllerProxy",
        "Server": "$ServerProxy",
        "Model3D": "$Model3D",
        "Clear": "/#00FF00"
      }
    },
    "Navigate": {
      "Module": "xVR.Remote.NavTerrain",
      "Source": "xVR",
      "Par": {
        "View": "$View3D"
      }
    },
    "SceneEditor": {
      "Module": "xVR.EVE.Remote",
      "Source": "xVR",
      "Par": {
        "View": "$View3D",
        "Controller": "$ControllerProxy"
      }
    }
  }
}