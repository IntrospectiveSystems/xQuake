{
	"Sources": {
		"xQuake": "{xGraph}",
		"xVR": "{xVR}"
	},
	"Modules": {
		"Orca": {
			"Module": "xVR.Orca.Daemon",
			"Source": "xVR",
			"Par": {
				"AgentServer": "$AgentServer",
				"SystemDir": "{cwd}",
				"Deploy": {
					"Name": "EVE",
					"Agent": "xQuake.Web.Flat.EVE",
					"SceneGraph":"{System}/scene.json",
					"Args": {
						"WebPort": 8080
					}
				},
				"Services": [
					"$Mesh"
				]
			}
		},
		"Mesh": {
			"Module":"xVR.Orca.Mesh",
			"Source":"xVR",
			"Par": {
			}
		},
		"AgentServer": {
			"Module": "xVR.Orca.AgentServer",
			"Source": "xVR",
			"Par": {
				"Agents": "{Agents}"
			}
		},
		"Deferred":[
		]
	}
}