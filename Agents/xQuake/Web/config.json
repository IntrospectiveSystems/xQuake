{
	"Sources": {
		"xGraph": "{xGraph}",
		"xVR": "{xVR}"
	},
	"Modules": {
		"Controller": {
			"Module": "xVR.EVE.Server",
			"Source":"xVR",
			"Par":{
				"Agent": "$Agent",
				"Services": [
					"$Agent"
				],
				"Server":"$EVE3DProxy",
				"ModelServer": "$ModelServer",
				"SceneGraph": "scene.json",
				"CWD":"{cwd}"
			}
		},
		"ControllerLink": {
			"Module":"xGraph.ServerProxy",
			"Source":"xGraph",
			"Par":{
				"Link": "$Controller",
				"Chan": "Controller",
				"Role": "Server",
				"Server":"$Server"
			}
		},
		"EVE3DProxy": {
			"Module":"xGraph.ServerProxy",
			"Source":"xGraph",
			"Par":{
				"Chan": "EVE3D",
				"Role": "Client",
				"Server":"$Server"
			}
		},
		"ModelServer": {
			"Module":"xVR.Model.Server",
			"Source":"xVR",
			"Par":{
				"Archive":"{Models}"
			}
		},
		"Deferred":[
		]
	}
}